<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Layouts2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Layouts2"
    Title="Layouts2">


    




    <TabBar>

        <Tab Title="Home" Icon="dotnet_bot.png">

            <ShellContent
            Title="Home"
             ContentTemplate="{DataTemplate local:MainPage}"
             Route="MainPage" />
        </Tab>

        <Tab Title="Pagina 1 " Icon="dotnet_bot.png">

            <ShellContent
    Title="Pagina 1 "
     ContentTemplate="{DataTemplate local:NewPage1}"
     Route="MainPage" />
        </Tab>

        <Tab Title="Pagina 2" Icon="dotnet_bot.png">

            <ShellContent
    Title="Pagina 2"
     ContentTemplate="{DataTemplate local:NewPage2}"
     Route="MainPage" />
        </Tab>
        <Tab Title="Pagina 3" Icon="dotnet_bot.png">

            <ShellContent
    Title="Pagina 3"
     ContentTemplate="{DataTemplate local:NewPage3}"
     Route="MainPage" />
        </Tab>

    </TabBar>

</Shell>

    
    